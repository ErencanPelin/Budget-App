<UserControl x:Class="BudgetApp.MVVM.View.BudgetView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BudgetApp.MVVM.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.Resources>
        <CollectionViewSource x:Key="expenseViewSource"/>
        <CollectionViewSource x:Key="expenseProductsViewSource" 
                              Source="{Binding Expenses, Source={StaticResource expenseViewSource}}"/>
    </UserControl.Resources>
    <Grid>
      
    </Grid>
</UserControl>
